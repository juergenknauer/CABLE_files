&CompileObservations_Namelist
! 
! Set path for observation files (need / on end)
  OBS_PATH = "/flush1/projects/oa_carbon/BIOS3/mdf/obs/CompileObservations/"
!
! Select model output file and resolution
  FILE_NAME = "/flush1/tru034/CABLE/mdf_test/bios_out_cable_1990_2017.nc"
  modelres = 0.05
!
! Select POP ini file (only used for Auscover Biomass Plot Library obs
  POPfile = "/flush1/tru034/CABLE/mdf_test/pop_bios_ini_1900_1989.nc"
!
! Select files for site selection for OzFlux, CosmOz and Catchments
  OzFluxSiteFile = "/flush1/projects/oa_carbon/BIOS3/mdf/obs/CompileObservations/OzFluxSites_test.txt"
  CosmOzSiteFile = "/flush1/projects/oa_carbon/BIOS3/mdf/obs/CompileObservations/CosmOzSites_test.txt"
  OzNetSiteFile = "/flush1/projects/oa_carbon/BIOS3/mdf/obs/CompileObservations/OzNetSiteList.csv"
  CatchmentFile = "/flush1/projects/oa_carbon/BIOS3/mdf/obs/CompileObservations/Catchments_test.txt"
!
! Climate File (for biome, which determines scaling of POP forest variables from patch to gridcell
  ClimateFile = "/data/tru034/LUH2/climate_from_cable/mdf_test/climate_rst_bios.nc"
!
! Select which observation types will be included in the observation file
! They can be used for optimisation or not, but must be in the model output if selected
! Options are 'opt' to use in optimisation, 'com' to compare only (weight=0), or
! 'not' to exclude from observation file
! Also select whether soil moisture and streamflow are daily or monthly obs
  EvTSelect =      'not'    ! ET evapotranspiration
  GPPSelect =      'not'    ! GPP gross primary production
  NEPSelect =      'not'    ! NEP net ecosystem production
  RecSelect =      'not'    ! Rec ecosystem respiration
  VRNPPSelect =    'not'    ! VAST-Raison leaf NPP
  VRSoilC0Select = 'not'    ! VAST-Raison SoilC0
  VRPhySelect =    'not'    ! VAST-Raison above-ground phytomass
  VRLitSelect =    'not'    ! VAST-Raison aboveground litter
  SMCSelect =      'not'    ! CosmOz soil moisture
  SMOSelect =      'opt'    ! OzNet soil moisture
  STRSelect =      'not'    ! Streamflow
  LBASelect =      'not'    ! Auscover Biomass Plot library - live basal area
  LTDSelect =      'not'    ! Auscover Biomass Plot library - live tree density
  AGDSelect =      'not'    ! Auscover Biomass Plot library - above-ground drymass
!
  SMCdm = 'not'        ! 'day' or 'mon' for CosmOz soil moisture  - check monthly, not implemented yet
  SMOdm = 'day'        ! 'day' or 'mon' for OzNet soil moisture  - check monthly, not implemented yet
  STRdm = 'not'        ! 'day' or 'mon' for streamflow  - check monthly, not implemented yet
/

